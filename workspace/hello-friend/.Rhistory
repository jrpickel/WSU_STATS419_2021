letter_count = function(string){
string = str_to_lower(string)
dict = str_count(string,letters)
num_punc = str_count(string,c("[:digit:]","[:punct:]"))
names(dict) = paste(letters)
dict$OTHER = sum(num_punc)
df = as.data.frame(dict)
df
}
