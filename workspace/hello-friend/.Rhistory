normalize = function(df){
row.names(df) = NULL
cols = colnames(df)
cols = cols[6:ncol(df)]
for (i in cols){
min_ = min(df[,i])
max_ = max(df[,i])
df[i] = (df[,i]-min_)/(max_-min_)
}
rank = c()
for (j in c(1:nrow(df))){
count = sum(df[j,c(6:ncol(df))],na.rm=TRUE)
rank = append(rank,count)
}
df$Rank = rank
df[order(-df$Rank),]
}
